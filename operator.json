{
  "name": "t Test",
  "description": "Tests for significant differences between the means of multiple groups.",
  "tags": [
    "statistical testing"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/t_test_operator"
  ],
  "container": "tercen/t_test_operator:latest",
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Row",
            "type": "",
            "description": "Optional row stratification factor",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "0..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Column",
            "type": "",
            "description": "Optional column stratification factor",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "0..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Measurement",
                "type": "numeric",
                "description": "Values to test",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              },
              {
                "kind": "MetaFactor",
                "name": "Group",
                "type": "",
                "description": "Group factor to compare (required)",
                "ontologyMapping": "group",
                "crosstabMapping": "color",
                "cardinality": "1..n",
                "factors": []
              },
              {
                "kind": "MetaFactor",
                "name": "Pairing",
                "type": "",
                "description": "Optional pairing factor for paired tests",
                "ontologyMapping": "pairing",
                "crosstabMapping": "label",
                "cardinality": "0..1",
                "factors": []
              }
            ]
          }
        ]
      }
    ],
    "outputSpecsV2": [
      {
        "kind": "ConditionalJoinSpec",
        "description": "Output varies based on 'detailed' property",
        "alternatives": [
          {
            "kind": "OutputAlternative",
            "condition": "detailed = false (default)",
            "joinSpec": {
              "kind": "OperatorJoinSpec",
              "joinOperators": [
                {
                  "kind": "JoinOperator",
                  "joinType": "left",
                  "leftPair": {
                    "kind": "ColumnPair",
                    "lColumns": [
                      "Row",
                      "Column"
                    ],
                    "rColumns": [
                      "Row",
                      "Column"
                    ]
                  },
                  "rightRelation": {
                    "kind": "TableRelation",
                    "attributes": [
                      {
                        "kind": "Attribute",
                        "name": "group1",
                        "type": "string"
                      },
                      {
                        "kind": "Attribute",
                        "name": "group2",
                        "type": "string"
                      },
                      {
                        "kind": "Attribute",
                        "name": "n1",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "n2",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "statistic",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "df",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "p",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "p.adj",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "p.adj.signif",
                        "type": "string"
                      },
                      {
                        "kind": "Attribute",
                        "name": "neglog10_p",
                        "type": "double"
                      }
                    ]
                  }
                }
              ]
            }
          },
          {
            "kind": "OutputAlternative",
            "condition": "detailed = true",
            "joinSpec": {
              "kind": "OperatorJoinSpec",
              "joinOperators": [
                {
                  "kind": "JoinOperator",
                  "joinType": "left",
                  "leftPair": {
                    "kind": "ColumnPair",
                    "lColumns": [
                      "Row",
                      "Column"
                    ],
                    "rColumns": [
                      "Row",
                      "Column"
                    ]
                  },
                  "rightRelation": {
                    "kind": "TableRelation",
                    "attributes": [
                      {
                        "kind": "Attribute",
                        "name": "group1",
                        "type": "string"
                      },
                      {
                        "kind": "Attribute",
                        "name": "group2",
                        "type": "string"
                      },
                      {
                        "kind": "Attribute",
                        "name": "n1",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "n2",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "statistic",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "df",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "p",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "p.adj",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "p.adj.signif",
                        "type": "string"
                      },
                      {
                        "kind": "Attribute",
                        "name": "neglog10_p",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "delta",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "estimate1",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "estimate2",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "conf.low",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "conf.high",
                        "type": "double"
                      }
                    ]
                  }
                }
              ]
            }
          }
        ]
      }
    ]
  },
  "properties": [
    {
      "kind": "EnumeratedProperty",
      "name": "alternative",
      "defaultValue": "two.sided",
      "values": [
        "two.sided",
        "less",
        "greater"
      ],
      "description": "A character string specifying the alternative hypothesis."
    },
    {
      "kind": "BooleanProperty",
      "name": "paired",
      "defaultValue": false,
      "description": "A logical variable indicating whether the observations are paired. If true, a label is expected from the Tercen projection to assign the pairs."
    },
    {
      "kind": "EnumeratedProperty",
      "name": "p.adjust.method",
      "defaultValue": "holm",
      "values": [
        "holm",
        "hochberg",
        "hommel",
        "bonferroni",
        "BH",
        "BY",
        "fdr",
        "none"
      ],
      "description": "Multiple testing correction to use."
    },
    {
      "kind": "DoubleProperty",
      "name": "mu",
      "defaultValue": 0,
      "description": "A number indicating the true value of the mean (or difference in means if you are performing a two sample test)"
    },
    {
      "kind": "BooleanProperty",
      "name": "var.equal",
      "defaultValue": false,
      "description": "A logical variable indicating whether to treat the two variances as being equal. If TRUE then the pooled variance is used to estimate the variance otherwise the Welch (or Satterthwaite) approximation to the degrees of freedom is used."
    },
    {
      "kind": "DoubleProperty",
      "name": "conf.level",
      "defaultValue": 0.95,
      "description": "Confidence level of the interval."
    },
    {
      "kind": "BooleanProperty",
      "name": "detailed",
      "defaultValue": false,
      "description": "Whether to produce a more detailed output or not."
    },
    {
      "kind": "DoubleProperty",
      "name": "reference_index",
      "defaultValue": 0,
      "description": "Index of the group used as reference. If 0 (default), this setting will be ignored."
    }
  ]
}